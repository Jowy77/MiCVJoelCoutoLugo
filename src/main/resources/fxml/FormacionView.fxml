<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<HBox fx:id="view" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="500.0" prefWidth="500.0" spacing="5.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1">
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
   <children>
      <TableView fx:id="formacionTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="desdeColumn" maxWidth="1.7976931348623157E308" minWidth="75.0" prefWidth="-1.0" text="Desde" />
          <TableColumn fx:id="hastaColumn" maxWidth="1.7976931348623157E308" minWidth="75.0" prefWidth="-1.0" text="Hasta" />
            <TableColumn fx:id="denominacionColumn" maxWidth="1.7976931348623157E308" minWidth="100.0" prefWidth="-1.0" text="Denominacion" />
            <TableColumn fx:id="organizadorColumn" maxWidth="1.7976931348623157E308" minWidth="100.0" prefWidth="-1.0" text="Organizador" />
        </columns>
      </TableView>
      <VBox spacing="5.0">
         <children>
            <Button fx:id="añadirButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onAñadirButtonAction" text="Añadir" />
            <Button fx:id="quitarButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onQuitarButtonAction" text="Eliminar" />
         </children>
      </VBox>
   </children>
</HBox>
